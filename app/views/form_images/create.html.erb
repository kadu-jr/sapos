<html>
  <body>
    <script type='text/javascript' charset='utf-8'>
      debugger;
      var loc = document.location;
      with(window.parent) {
          setTimeout(function() {
              try {
                  var code = `<tr class="record  " id="as_form_images-list-<%= @image[:id] %>-row" data-refresh="<%= @image[:id] %>">
                                <td class="name-column sorted  ">      <%= @image[:name] %></td>
                                <td class="text-column">      <%= @image[:text] %></td>
                                <td class="actions">
                                  <table>
                                    <tr>
                                      <td class="indicator-container">
                                        <img style="visibility:hidden;" id="as_form_images-record-<%= @image[:id] %>-loading-indicator" alt="loading indicator" class="loading-indicator" src="/assets/active_scaffold/indicator-07b202d8027099d3add8213095a203cecb4938acdebc80f755ef2251a1a5b880.gif" />
                                      </td>
                                      <td>
                                        <a class="edit as_action" data-position="replace" data-action="edit" id="as_form_images-edit-<%= @image[:id] %>-link" data-remote="true" href="/form_images/<%= @image[:id] %>/edit?utf8=%E2%9C%93">
                                          <i title='Editar' class='fa fa-pencil'></i>
                                        </a>
                                      </td>
                                      <td>
                                        <a class="destroy as_action" data-confirm="Tem certeza que deseja excluir?" data-action="destroy" id="as_form_images-destroy-<%= @image[:id] %>-link" data-remote="true" rel="nofollow" data-method="delete" href="/form_images/<%= @image[:id] %>?destroy_action=true&amp;utf8=%E2%9C%93">
                                          <i title='Excluir' class='fa fa-trash-o'></i>
                                        </a>
                                      </td>
                                      <td>
                                        <a class="show as_action" data-position="replace" data-action="show" id="as_form_images-show-<%= @image[:id] %>-link" data-remote="true" href="/form_images/<%= @image[:id] %>?utf8=%E2%9C%93">
                                          <i title='Visualizar' class='fa fa-eye'></i>
                                        </a>
                                      </td>
                                    </tr>
                                  </table>
                                </td>
                              </tr>`
                  var action_link;
                  action_link = ActiveScaffold.find_action_link('as_form_images-create--form');
                  if (action_link) action_link.update_flash_messages('');
                  ActiveScaffold.create_record_row(action_link ? action_link.scaffold() : 'as_form_images-active-scaffold', code, {"insert_at":"top"});
                  if (action_link) action_link.close();
              }
              catch (e) {
                  alert('RJS error:' + e.toString());
              };
              window.loc && loc.replace('about:blank');
              }, 1) }
</script></body></html>