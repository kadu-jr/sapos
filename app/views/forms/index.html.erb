<div id="as_forms-active-scaffold" class="active-scaffold active-scaffold-as_forms  forms-view default-theme">
  <div class="active-scaffold-header">
    <div class="actions">
      <a class="new as_action" data-position="top" data-action="new" id="as_forms-new--link" data-remote="true" href="/forms/new">Adicionar</a><a class="show_search as_action" data-position="top" data-action="show_search" id="as_forms-show_search--link" data-remote="true" href="/forms/show_search">Buscar</a>
      <img style="visibility:hidden;" id="as_forms-table--loading-indicator" alt="loading indicator" class="loading-indicator" src="/assets/active_scaffold/indicator-07b202d8027099d3add8213095a203cecb4938acdebc80f755ef2251a1a5b880.gif">
    </div>
    <h2>Relatórios</h2>

  </div>
  <table>
    <tbody class="before-header" id="as_forms-search-container">
    <tr><td></td></tr>
    </tbody>
  </table>
  <div id="as_forms-content" class="as_content">
    <table>
      <thead>
      <tr>
        <th id="as_forms-nome-column" class="nome-column_heading "><a class="as_sort" data-page-history="as_forms" data-remote="true" data-method="get" href="/forms?page=1&amp;sort=nome&amp;sort_direction=ASC">Nome</a></th>
        <th id="as_forms-descricao-column" class="descricao-column_heading "><a class="as_sort" data-page-history="as_forms" data-remote="true" data-method="get" href="/forms?page=1&amp;sort=descricao&amp;sort_direction=ASC">Descrição</a></th>
        <th id="as_forms-query-column" class="query-column_heading "><p>Consulta</p></th>
        <th id="as_forms-template-column" class="template-column_heading "><p>Template</p></th>
        <th class="actions">
        </th>

      </tr>
      </thead>
      <tbody class="messages">
      <tr class="record even-record">
        <td colspan="3" class="messages-container">
          <p class="error-message message server-error" style="display:none;">
            Internal Error
            <a href="#" class="close" title="Fechar">Fechar</a>
          </p>
          <div id="as_forms-messages" class="action-messages">

          </div>
          <div class="filtered-message" style="display:none;" data-search=""></div>      <p id="as_forms-empty-message" class="empty-message" style="display:none;">
          Nenhum Dado Cadastrado
        </p>
        </td>
      </tr>
      </tbody>

      <tbody class="records" id="as_forms-tbody" data-refresh-record="/forms/--ID--/list">
      <% @forms.each_with_index do |form,index | %>

        <tr class="record  <% if (index%2 == 1)  %> <%= "even-record" %> <%end %>" id="as_forms-list-<%= form.id %>-row" data-refresh="<%= form.id %>" style="">

          <td class="nome-column  ">
            <%= form.nome %>
          </td>
          <td class="descricao-column  ">
            <%= form.descricao %>
          </td>
          <td class="query-column  ">
            <a class="query edit as_action" data-position="after" data-action="edit" id="as_forms-__queries-edit-query-<%= form.query.id %>-<%= form.id %>-link" data-remote="true" href="/queries/<%= form.query_id %>/edit?association=query&amp;form_id=<%= form.id %>&amp;parent_scaffold=forms&amp;utf8=%E2%9C%93"><%= form.query.name %></a>
          </td>
          <td class="template-column  ">
            <a class="template edit as_action" data-position="after" data-action="edit" id="as_forms-__form_templates-edit-template-<%= form.template.id %>-<%= form.id %>-link" data-remote="true" href="/form_templates/<%= form.template.id %>/edit?association=template&amp;form_id=<%= form.id %>&amp;parent_scaffold=forms&amp;utf8=%E2%9C%93"><%= form.template.name %></a>
          </td>
          <td class="actions"><table>
            <tbody><tr>
              <td class="indicator-container">
                <img style="visibility:hidden;" id="as_forms-record-4-loading-indicator" alt="loading indicator" class="loading-indicator" src="/assets/active_scaffold/indicator-07b202d8027099d3add8213095a203cecb4938acdebc80f755ef2251a1a5b880.gif">
              </td>
              <td>
                <a class="consult as_action" data-position="after" data-action="consult" id="as_forms-consult-<%= form.id %>-link" data-remote="true" href="/forms/<%= form.id %>/consult?utf8=%E2%9C%93">
                  <i title="Executar" class="fa fa-table"></i>
                </a>
              </td>
              <td>
                <a class="edit as_action" data-position="replace" data-action="edit" id="as_forms-edit-<%= form.id %>-link" data-remote="true" href="/forms/<%= form.id %>/edit?utf8=%E2%9C%93">
                  <i title="Editar" class="fa fa-pencil"></i>
                </a>
            </td>
              <td>
                <a class="destroy as_action" data-confirm="Tem certeza que deseja excluir?" data-action="destroy" id="as_forms-destroy-<%= form.id %>-link" data-remote="true" rel="nofollow" data-method="delete" href="/forms/<%= form.id %>?destroy_action=true&amp;utf8=%E2%9C%93">
                  <i title="Excluir" class="fa fa-trash-o"></i>
                </a>
              </td>
              <td>
                <a class="show as_action" data-position="replace" data-action="show" id="as_forms-show-<%= form.id %>-link" data-remote="true" href="/forms/<%= form.id %>?utf8=%E2%9C%93">
                  <i title="Visualizar" class="fa fa-eye"></i>
                </a>
              </td>
            </tr>
            </tbody></table></td>
        </tr>
      <% end %>





      </tbody>
    </table>
    <div class="active-scaffold-footer">
      <div class="active-scaffold-found"><span class="active-scaffold-records"><%= @forms.count %></span> Registros Encontrados</div>
      <div class="active-scaffold-pagination">

      </div>
      <br><!-- to clear this block -->
    </div>


  </div>
</div>